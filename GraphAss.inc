; --------------------------
; Purpose:
; To make it easy to find where to draw graphics
; --------------------------
; How:
; Because each square is 25x25 pixels, each time we multiply by 25 either the X or Y
; It'll move a square in that value on screen
;
; Parameters: boardX = the X coordinates in the board, BoardY = the Y coordinates in the board
;
; Returns: CX = the resulting X screen coordinate, DX = the resulting Y screen coordinate
; --------------------------
proc convertFromBoardToScreen
    @@boardX equ [word ptr bp + 6]
    @@boardY equ [word ptr bp + 4]
    push bp
    mov bp, sp
    ; We'll convert X and Y coordinates on the board to X and Y coordinates on the screen
    mov cx, 1
    imul cx, @@boardX, 25
    mov dx, 1
    imul dx, @@boardY, 25
    pop bp
    ret 4
endp convertFromBoardToScreen

proc drawBoard
    push bp
    mov bp, sp
    push ax
    push bx
    push cx
    push dx
    sub sp, 4

    @@cordX equ [word ptr bp - 2]
    @@cordY equ [word ptr bp - 4]

    mov @@cordY, 0
    mov ax, 0 ; Will be used to choose the color

    mov cx, 8
    @@draw:
        mov dx, 8
        mov @@cordX, 0
        @@drawRow:
            inc ax
            mov bx, ax
            and bx, 0001b ; BX = 1 if odd, BX = 0 if even
            mov bl, [byte ptr bx + squareColors] ; Get the corrosponding color
            xor bh, bh

            push bx
            push @@cordX
            push @@cordY
            call drawSquare
            inc @@cordX
            dec dx
            cmp dx, 0
            jne @@drawRow
        inc @@cordY
        inc ax
        loop @@draw
    pop dx
    pop cx
    pop bx
    pop ax
    add sp, 4
    pop bp
    ret
endp drawBoard

proc drawSquare
    @@color equ [word ptr bp + 8]
    @@cordX equ [word ptr bp + 6]
    @@cordY equ [word ptr bp + 4]

    push bp
    mov bp, sp
    push ax
    push cx
    push dx
    push di

    
    push @@cordX
    push @@cordY
    call convertFromBoardToScreen

    mov di, 320 ; DI will be used to access the Screen's memory
    imul di, dx ; Screen offset is calculated with:
    add di, cx ; (320 * Y) + X

    mov ax, @@color

    mov cx, assetSize
    @@draw:
        mov dx, assetSize
        @@drawRow:
            mov [es:di], al
            inc di
            dec dx
            cmp dx, 0
            jne @@drawRow
        add di, 320 ; Goes down a line
        sub di, assetSize ; Returns to starting X position
        loop @@draw
    
    pop di
    pop dx
    pop cx
    pop ax
    pop bp
    ret 6
endp drawSquare

proc drawHighlight
    @@color equ [word ptr bp + 8]
    @@boardX equ [word ptr bp + 6]
    @@boardY equ [word ptr bp + 4]

    push bp
    mov bp, sp
    push ax
    push cx
    push dx
    push di

    push @@boardX
    push @@boardY
    call convertFromBoardToScreen

    mov di, 320 ; DI will be used to access the Screen's memory
    imul di, dx ; Screen offset is calculated with:
    add di, cx ; (320 * Y) + X

    mov ax, @@color

    mov cx, 24
    @@topSide:
        mov [byte ptr es:di], al
        inc di ; Moves to the next pixel
        loop @@topSide
    
    mov cx, 24
    @@rightSide:
        mov [byte ptr es:di], al
        add di, 320 ; Goes down a line
        loop @@rightSide

    mov cx, 24
    @@bottomSide:
        mov [byte ptr es:di], al
        dec di ; Moves to the previous pixel
        loop @@bottomSide
    
    mov cx, 24
    @@leftSide:
        mov [byte ptr es:di], al
        sub di, 320 ; Goes up a line
        loop @@leftSide
    pop di
    pop cx
    pop dx
    pop ax
    pop bp
    ret 4
endp drawHighlight

proc initializeBoard
    @@boardX equ [word ptr bp - 2]
    @@boardY equ [word ptr bp - 4]

    push bp
    mov bp, sp
    sub sp, 4
    push ax
    push bx
    push cx
    push dx
    push di
    push si

    xor ah, ah
    mov bx, offset board
    mov si, 0
    mov @@boardY, 0

    mov cx, 8
    @@goThroughBoard:
        mov @@boardX, 0
        mov dx, 8
        @@goThroughRow:
            mov al, [byte ptr bx + si] ; Puts the value of the piece so the "drawPiece" procedure can pick the desired Array
            cmp al, 0
            je @@sof ; If the spot is empty
            push ax ; Push the piece's value
            push @@boardX
            push @@boardY
            call drawPiece
            @@sof:
            inc si
            add @@boardX, 1
            dec dx
            cmp dx, 0
            jne @@goThroughRow

        add @@boardY, 1
        loop @@goThroughBoard
    pop si
    pop di
    pop dx
    pop cx
    pop bx
    pop ax
    add sp, 4
    pop bp
    ret
endp initializeboard

proc drawPiece
    @@PieceValue equ [word ptr bp + 8]
    @@boardX equ [word ptr bp + 6]
    @@boardY equ [word ptr bp + 4]

    push bp
    mov bp, sp
    push ax
    push cx
    push dx
    push di
    push si

    mov ax, @@PieceValue
    cmp al, 0
    jg @@whitePiece
    jl @@blackPiece
    ; If for some reason it's 0 (meaning EMPTY) then simply don't draw
    je @@exit
    @@blackPiece:
        neg al ; We put black pieces as negative values so this converts it back
        mov si, ax
        mov si, [word ptr blackPieces + si] ; Gets the offset of the wanted black piece
        jmp @@sof
    @@whitePiece:
        mov si, ax
        mov si, [word ptr whitePieces + si] ; Gets the offset of the wanted white piece
    @@sof:
    ; Now SI will point to the desired piece's array offset

    push @@boardX
    push @@boardY
    call convertFromBoardToScreen

    mov di, 320 ; DI will be used to access the Screen's memory
    imul di, dx ; Screen offset is calculated with:
    add di, cx ; (320 * Y) + X

    mov cx, assetSize ; Height of the Array
    @@draw:
        mov dx, assetSize ; Width of the Array
        @@drawRow:
            cmp [byte ptr si], 55h ; We don't want to draw this color (we'll treat it like transparent)
            je @@next
            mov al, [byte ptr si] ; Move the value of the wanted element into AX
            mov [es:di], al ; Put the value on the screen
            @@next:
            inc di ; Move to next pixel on the screen
            inc si ; Move to next element in the Asset's Array
            dec dx
            cmp dx, 0
            jne @@drawRow
        
        add di, 320 ; Goes down a line on the screen
        sub di, assetSize ; Returns to starting X position
        loop @@draw
    @@exit:
    pop si
    pop di
    pop dx
    pop cx
    pop ax
    pop bp
    ret 6
endp drawpiece

; --------------------------
; Purpose:
; To give a visual indication to whose turn it is (White or Black)
; --------------------------
; How:
; checks which player's turn it is, and jumps to his section
; then it clears the previous location it was (to delete the arrow)
; And draws the turn indicator (the arrow) at the needed position
;
; Parameters: None
;
; Returns: None
; --------------------------
proc indicatePlayerTurn
    push ax
    push cx
    push dx
    push di
    push si

    lea si, [TurnIndicator]

    cmp [playerTurn], 0
    jl @@isBlackTurn

    @@isWhiteTurn:
        ; We'll clear the previous arrow
        push 00h
        push 8
        push 0
        call drawSquare
        ; And now we'll get the desired position to show the arrow
        push 8
        push 7
        call convertFromBoardToScreen
        jmp @@sof
    @@isBlackTurn:
        ; We'll clear the previous arrow
        push 00h
        push 8
        push 7
        call drawSquare
        ; And now we'll get the desired position to show the arrow
        push 8
        push 0
        call convertFromBoardToScreen
    @@sof:
    mov di, 320 ; DI will be used to access the Screen's memory
    imul di, dx ; Screen offset is calculated with:
    add di, cx ; (320 * Y) + X

    mov cx, assetSize ; Height of the Array
    @@draw:
        mov dx, assetSize ; Width of the Array
        @@drawRow:
            cmp [byte ptr si], 55h ; We don't want to draw this color (we'll treat it like transparent)
            je @@next
            mov al, [byte ptr si] ; Move the value of the wanted element into AX
            mov [es:di], al ; Put the value on the screen
            @@next:
            inc di ; Move to next pixel on the screen
            inc si ; Move to next element in the Asset's Array
            dec dx
            cmp dx, 0
            jne @@drawRow
        
        add di, 320 ; Goes down a line on the screen
        sub di, assetSize ; Returns to starting X position
        loop @@draw
    
    pop si
    pop di
    pop dx
    pop cx
    pop ax
    ret
endp indicatePlayerTurn

