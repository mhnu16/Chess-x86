proc printColoredText
    @@stringOffset equ [word ptr bp + 10]
    @@color equ [byte ptr bp + 8]
    @@textY equ [byte ptr bp + 6]
    @@textX equ [byte ptr bp + 4]
    push bp
    mov bp, sp
    push ax
    push bx
    push cx
    push si

    mov ah, 2h ; Set text print position
    mov bh, 0
    mov dh, @@textY
    mov dl, @@textX
    int 10h

    mov ah, 9h
    mov bh, 0h
    mov bl, @@color

    mov si, @@stringOffset
    @@loopOverString:
        cmp [byte ptr si], 0
        je @@sof
        mov ah, 9h
        mov bh, 0h
        mov bl, @@color
        mov cx, 1
        mov al, [si]
        int 10h
        inc dl ; Increment X
        mov ah, 2h ; Set text print position
        mov bh, 0
        mov dh, @@textY
        int 10h
        inc si
        jmp @@loopOverString
        

    @@sof:
    pop si
    pop cx
    pop bx
    pop ax
    pop bp
    ret 8
endp printColoredText